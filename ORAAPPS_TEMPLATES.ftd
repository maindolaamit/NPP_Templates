                                        , p_node_type              => 'FUNCTION'
                                        , p_pkg_name               => g_package_name
                                        , p_pkg_sub_program_name   => g_proc_name
                                        , p_message                => p_message
                                         );

   END debug_log;

   $[0[]0]
      

END $[![package]!];
/

