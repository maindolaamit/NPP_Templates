eg_utlfile_proc
Lang:SQL
   --
   --
   -- +================================================================================+
   -- | Name :       CALCULATE_TDGGETPR_PROC                                           |
   -- | Description: Private PROCEDURE                                                 |
   -- |                                                                                |
   -- | In Parameters :     p_in_chr_brand         IN     VARCHAR2                     |
   -- |                     p_in_chr_dealer_num    IN     VARCHAR2                     |
   -- |                     p_in_chr_model         IN     VARCHAR2                     |
   -- |                     p_in_num_org_id        IN     NUMBER                       |
   -- |                                                                                |
   -- | Out Parameters :    p_out_chr_errmsg          OUT VARCHAR2                     |
   -- |                     p_out_num_retcode         OUT NUMBER                       |
   -- |                     p_out_num_pr_percent      OUT NUMBER                       |
   -- |                                                                                |
   -- |                                                                                |
   -- +================================================================================+
   --
   --
   PROCEDURE xxecs_pos_serial_proc(
      p_out_chr_errmsg            OUT VARCHAR2
     ,p_out_num_retcode           OUT NUMBER
     ,p_in_date_extract_date   IN     DATE
     ,p_in_chr_directory       IN     VARCHAR
     ,p_in_chr_file_name       IN     VARCHAR
   )
   IS
      l_chr_errmsg                                 VARCHAR2( 4000 ) := g_chr_success;
      l_num_ret_code                               NUMBER := g_num_success_code;
      l_chr_file_header                            VARCHAR2( 4000 );
      l_file_handler                               UTL_FILE.file_type;

      -- Cursor to fetch POS Serial details
      CURSOR cur_fetch_pos_serial_details
      IS
         SELECT    '"'
                || header_record_id
                || '","'
                || line_record_id
                || '","'
                || so_header_id
                || '","'
                || so_line_id
                || '","'
                || serial_number
                || '","'
                || p_in_date_extract_date
                || '"'
                   pos_serial_details
           FROM apps.xxecs_pos_serial_st;
   BEGIN
      -- Initialize the out parameter with Default values
      p_out_chr_errmsg    := l_chr_errmsg;
      p_out_num_retcode   := l_num_ret_code;
      -- Define file header
      l_chr_file_header      :=
         '"HEADER_RECORD_ID"' || ',"LINE_RECORD_ID"' || ',"SO_HEADER_ID"' || ',"SO_LINE_ID"' || ',"SERIAL_NUMBER"' || ',"EXTRACTION DATE"';
      -- Open the file handler for file
      xxecs_pos_conv_log_proc( '   Creating File ' || p_in_chr_file_name );
      l_file_handler      :=
         UTL_FILE.fopen(
                         p_in_chr_directory
                        ,p_in_chr_file_name
                        ,'w'
                        ,32767
                        );

      -- Check if file has been created or not
      IF UTL_FILE.is_open( l_file_handler )
      THEN
         xxecs_pos_conv_log_proc( '   File created successfully.' );
         xxecs_pos_conv_log_proc( '   Writting Records in the file.' );
         -- Insert Header into file
         UTL_FILE.put_line(
                            l_file_handler
                           ,l_chr_file_header
                           );

         -- Open cursor to fetch the comma separated records
         FOR cur_fetch_pos_srl_details_rec IN cur_fetch_pos_serial_details
         LOOP
            UTL_FILE.put_line(
                               l_file_handler
                              ,cur_fetch_pos_srl_details_rec.pos_serial_details
                              );
         END LOOP;
      ELSE
         l_num_ret_code   := g_num_failure_code;
         l_chr_errmsg     := 'Some Error occurred while opening the file. Error : ' || SQLERRM;
      END IF;

      xxecs_pos_conv_log_proc( '   Records Written successfully.' );
      -- Close the file
      UTL_FILE.fclose( l_file_handler );
      xxecs_pos_conv_log_proc( '   Closing File.' );
   EXCEPTION
      WHEN UTL_FILE.invalid_path
      THEN
         p_out_chr_errmsg    := p_in_chr_directory || ' : Invalid Oracle Path for the file. Error : ' || SQLERRM;
         p_out_num_retcode   := g_num_failure_code;
      WHEN OTHERS
      THEN
         p_out_chr_errmsg    := 'When others exception occured while POS Header file. Error : ' || SQLERRM;
         p_out_num_retcode   := g_num_failure_code;
   END xxecs_pos_serial_proc;
[>END<]!$[FingerTextData FingerTextData]@#
