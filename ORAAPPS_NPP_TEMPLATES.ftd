eg_bulk_update_proc
Lang:SQL
   --
   --
   -- +=============================================================================+
   -- | Name :       FETCH_CAR_DETAILS_PROC                                         |
   -- | Description: Private PROCEDURE                                              |
   -- |                                                                             |
   -- | In Parameters :     p_in_rec_line_info   IN            type_rec_line_info   |
   -- |                                                                             |
   -- |                                                                             |
   -- |                                                                             |
   -- |                                                                             |
   -- | Out Parameters :    p_out_rec_car_info      OUT NOCOPY type_rec_car_info    |
   -- |                     p_out_chr_errmsg        OUT NOCOPY VARCHAR2             |
   -- |                     p_out_num_retcode       OUT NOCOPY NUMBER               |
   -- |                                                                             |
   -- |                                                                             |
   -- +=============================================================================+
   --
   --

PROCEDURE xxecs_update_intf_table_proc(
   p_out_chr_errmsg            OUT VARCHAR2
  ,p_out_num_retcode           OUT NUMBER
  ,p_out_num_rows_updated      OUT NUMBER
  ,p_in_num_cur_flag        IN     NUMBER
  ,p_in_num_new_flag        IN     NUMBER
  ,p_in_chr_new_errmsg      IN     VARCHAR2
  ,p_in_num_batch_id        IN     NUMBER
)
IS
   l_chr_errmsg                                 VARCHAR2( 4000 ) := g_chr_success;
   l_num_ret_code                               NUMBER := g_num_success_code;
   l_proc_custom_exception                           EXCEPTION;
   l_tbl_pos_intf_hdr                           pos_intf_hdr;
BEGIN
   -- Initialize the out parameter with Default values
   p_out_chr_errmsg         := l_chr_errmsg;
   p_out_num_retcode        := l_num_ret_code;
   p_out_num_rows_updated   := 0;

   -- Bulk collect data
   BEGIN
      SELECT *
        BULK COLLECT INTO l_tbl_pos_intf_hdr
        FROM apps.xxecs_pos_intf_hdr_st
       WHERE process_flag = p_in_num_cur_flag
             AND batch_id = p_in_num_batch_id;
   EXCEPTION
      WHEN OTHERS
      THEN
         l_num_ret_code   := g_num_failure_code;
         l_chr_errmsg     := 'When others exception occurred while bulk collecting POS Interface table. Error : ' || SQLERRM;
         RAISE l_proc_custom_exception;
   END;

   BEGIN
      -- Update XXECS_POS_INTF_HDR_ST staging table
      FORALL indx IN 1 .. l_tbl_pos_intf_hdr.COUNT
         UPDATE apps.xxecs_pos_intf_hdr_st
            SET target1_process_flag        = p_in_num_new_flag
               ,target1_error_message       = p_in_chr_new_errmsg
               ,target1_request_id          = g_num_request_id
               ,creation_date               = g_date_sysdate
               ,created_by                  = g_num_user_id
               ,last_updated_by             = g_num_user_id
               ,last_update_date            = g_date_sysdate
               ,last_update_login           = g_num_login_id
               ,program_application_id      = g_num_program_appl_id
               ,program_id                  = g_num_program_id
               ,program_update_date         = g_date_sysdate
               ,target1_transmission_mode   = NVL( target1_transmission_mode, trans_mode_rec.transmission_mode )
          WHERE header_record_id = l_tbl_pos_intf_hdr( indx ).header_record_id
                AND batch_id = l_tbl_pos_intf_hdr( indx ).batch_id;

      -- Fetch rows updated
      p_out_num_rows_updated   := p_out_num_rows_updated + 1;
   EXCEPTION
      WHEN OTHERS
      THEN
         l_num_ret_code   := g_num_failure_code;
         l_chr_errmsg      :=
               'When others exception occurred while updating POS Header Interface table for Error Message : '
            || l_chr_errmsg
            || '. Error : '
            || SQLERRM;
         RAISE l_proc_custom_exception;
   END;
-- Exception Block of Procedure
EXCEPTION
   WHEN l_proc_custom_exception
   THEN
      p_out_num_retcode   := l_num_ret_code;
      p_out_chr_errmsg    := l_chr_errmsg;
   WHEN OTHERS
   THEN
      p_out_num_retcode   := g_num_failure_code;
      p_out_chr_errmsg    := 'When others exception occurred while updating POS Header Interface table. Error : ' || SQLERRM;
END xxecs_update_intf_table_proc;[>END<]!$[FingerTextData FingerTextData]@#
