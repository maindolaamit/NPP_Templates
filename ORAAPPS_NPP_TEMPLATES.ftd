   -- Get Hold reason code hold entity code for releasing
   BEGIN

      SELECT ohd.hold_id
        INTO p_hold_id
        FROM oe_hold_definitions ohd
           , fnd_lookup_values flv
       WHERE     1 = 1
             AND ohd.name = flv.meaning
             AND ohd.name = l_hold_name
             AND flv.lookup_type = 'EMR HOLD TYPE VLVS'
             --                AND flv.description = 'Order Holds'
             AND flv.language = USERENV( 'LANG' )
             AND SYSDATE BETWEEN NVL( flv.start_date_active, SYSDATE - 1 ) AND NVL( flv.end_date_active, SYSDATE + 1 );
   EXCEPTION
      WHEN OTHERS THEN
         l_chr_errmsg := 'Exception while getting hold id for l_hold_name : ' || l_hold_name;
         RAISE excp_custom;
   END;

   x_return_status := NULL;
   x_msg_count := NULL;
   x_msg_data := NULL;


   apps.oe_holds_pub.check_holds(
                                  p_api_version         => p_api_version
                                , p_init_msg_list       => p_init_msg_list
                                , p_commit              => p_commit
                                , p_validation_level    => p_validation_level
                                , p_header_id           => l_header_id
                                , p_line_id             => l_line_id
                                , p_hold_id             => p_hold_id
                                , p_wf_item             => NULL
                                , p_wf_activity         => NULL
                                , p_entity_code         => NULL
                                , p_entity_id           => NULL
                                , p_entity_code2        => NULL
                                , p_entity_id2          => NULL
                                , p_chk_act_hold_only   => 'N'
                                , x_result_out          => x_result_out
                                , x_return_status       => x_return_status
                                , x_msg_count           => x_msg_count
                                , x_msg_data            => x_msg_data
                                 );

   -- Check API return Status
   IF ( NVL( x_return_status, fnd_api.g_ret_sts_error ) <> fnd_api.g_ret_sts_success
       AND x_msg_count > 0 ) THEN

      -- Loop through message Count
      FOR i IN 1 .. x_msg_count
      LOOP
         fnd_msg_pub.get(
                          p_msg_index       => i
                        , p_encoded         => fnd_api.g_false
                        , p_data            => x_msg_data
                        , p_msg_index_out   => l_api_msg_index
                         );
         l_chr_errmsg :=
            SUBSTR(
                    ' Count : ' || l_api_msg_index || '. Message :  ' || x_msg_data
                  , 1
                  , 2000
                   );
      END LOOP;

      ROLLBACK;
      -- Print exception
      DBMS_OUTPUT.put_line( 'Line id : ' || l_line_id || '. Header id : ' || l_header_id || '. Error : ' || l_chr_errmsg );
   ELSE
      --DBMS_OUTPUT.put_line( 'Holds released Successfully.' );
      COMMIT;
   END IF;
EXCEPTION
   WHEN excp_custom THEN
      DBMS_OUTPUT.put_line( l_chr_errmsg );
   WHEN OTHERS THEN
      DBMS_OUTPUT.put_line( 'When others. Error : ' || SQLERRM );
END;
