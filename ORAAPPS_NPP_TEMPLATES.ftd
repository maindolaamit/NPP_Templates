apps_submit_request
Lang:SQL
/* Submit a Concurrent child request */
DECLARE
   l_child_request_id                       NUMBER := 0;
   l_parent_id                              NUMBER := TO_NUMBER( fnd_global.conc_request_id );
   l_interval                               NUMBER := 30;
   l_max_wait                               NUMBER := 0;
   l_boolean_status                             BOOLEAN;
   l_chr_phase                                  VARCHAR2( 2000 );
   l_chr_status                                 VARCHAR2( 2000 );
   l_chr_dev_phase                              VARCHAR2( 2000 );
   l_chr_dev_status                             VARCHAR2( 2000 );
   l_chr_messages                               VARCHAR2( 2000 );
BEGIN
   -- Submit request
   l_num_child_request_id      :=
      fnd_request.submit_request(
                                  'XXECS_FIN' --Application Short Name
                                 ,'XXAR_DNB_INBOUND_PROCESS_INT' -- Conc Program Short Name
                                 ,NULL --Description
                                 ,NULL --Start Time
                                 ,FALSE --Sub Request
                                 ,l_dnb_in_dir --Argument1
                                 ,l_dnb_log_file --Argument2
                                 );

   IF ( l_num_child_request_id <> 0 )
   THEN
      -- Make a parent child relationship
      BEGIN
         UPDATE fnd_concurrent_requests
            SET has_sub_request   = 'Y'
          WHERE request_id = l_num_parent_id;

         UPDATE fnd_concurrent_requests
            SET is_sub_request        = 'Y'
               ,priority_request_id   = l_num_parent_id
               ,parent_request_id     = l_num_parent_id
          WHERE request_id = l_num_child_request_id;

         COMMIT;
      EXCEPTION
         WHEN OTHERS
         THEN
            apps.fnd_file.put_line( apps.fnd_file.LOG, 'Error : ' || SQLERRM );
      END;
   END IF;

   -- Wait for Child request completeion
   l_boolean_status      :=
      fnd_concurrent.wait_for_request(
                                       l_num_child_request_id
                                      ,l_num_interval -- Interval between checking the status of program
                                      ,l_num_max_wait -- Max wait time 0 means forever
                                      ,l_chr_phase -- Program Phase
                                      ,l_chr_status -- Program Status
                                      ,l_chr_dev_phase -- COMPLETED
                                      ,l_chr_dev_status -- NORMAL
                                      ,l_chr_messages
                                      );
   -- Print completion status
   apps.fnd_file.put_line( apps.fnd_file.LOG, 'Phase  : ' || l_chr_dev_phase );
   apps.fnd_file.put_line( apps.fnd_file.LOG, 'Status : ' || l_chr_dev_status );
END;
